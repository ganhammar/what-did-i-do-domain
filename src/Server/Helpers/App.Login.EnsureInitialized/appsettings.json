{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "DynamoDB": {
    "ServiceUrl": ""
  },
  "ClientOptions": {
    "Clients": [
      {
        "ClientId": "what-did-i-do.authorizer",
        "DisplayName": "Authorizer",
        "Secret": "",
        "ConsentType": "systematic",
        "Permissions": [
          "ept:introspection",
          "ept:token",
          "gt:client_credentials"
        ]
      },
      {
        "ClientId": "what-did-i-do.account",
        "DisplayName": "Account",
        "ConsentType": "explicit",
        "RedirectUris": [
          "http://localhost:3000/login/callback",
          "https://www.wdid.fyi/account/login/callback",
          "http://localhost:3000/login/silent-renew",
          "https://www.wdid.fyi/account/login/silent-renew"
        ],
        "PostLogoutRedirectUris": [
          "http://localhost:3000/logout/callback",
          "https://www.wdid.fyi/account/logout/callback"
        ],
        "Permissions": [
          "ept:authorization",
          "ept:logout",
          "ept:token",
          "gt:authorization_code",
          "rst:code",
          "scp:email",
          "scp:profile",
          "scp:roles",
          "scp:account"
        ],
        "Requirements": [
          "ft:pkce"
        ]
      }
    ]
  },
  "ScopeOptions": {
    "Scopes": [
      {
        "Name": "account",
        "Resources": [
          "what-did-i-do.account"
        ]
      }
    ]
  }
}
